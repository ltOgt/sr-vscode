{
    "name": "small-read-highlighting",
    "displayName": "SmallRead Highlighting",
    "description": "TM Grammar for SmallRead",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sr",
                "aliases": [
                    "SR",
                    "SmallRead"
                ],
                "extensions": [
                    ".sr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sr",
                "scopeName": "source.sr",
                "path": "./syntaxes/sr.tmLanguage.json"
            }
        ]
    }
}